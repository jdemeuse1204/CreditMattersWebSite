  "version": "0.2.0",
  "configurations": [
    {
      "name": "Attach To Process",
      "type": "chrome",
      "request": "attach",
      "port": 9222,
      "webRoot": "${workspaceRoot}"
    },
    {
      "name": "Attach APQueryUI",
      "type": "chrome",
      "request": "attach",
      "port": 3000,
      "url":"http://localhost:3000",
      "webRoot": "${workspaceRoot}"
    },
    {
      "name": "Launch Website",
      "type": "chrome",
      "request": "launch",
      "url":"http://localhost:3000",
      "webRoot": "${workspaceRoot}",
      "skipFiles": [
        "*undefined*"
      ],
      "runtimeExecutable": "C:\\Users\\demejam\\AppData\\Local\\Google\\Chrome SxS\\Application\\chrome.exe"
    }
  ]
}
